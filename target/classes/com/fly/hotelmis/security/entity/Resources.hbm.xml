<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.fly.hotelmis.security.entity.Resources" table="resources"
		catalog="finance">
		<id name="id" type="java.lang.String" column="id" length="32">
			<generator class="uuid.hex" />
		</id>
		<property name="resoname" type="java.lang.String">
			<column name="resoname" length="32" />
		</property>
		<property name="type" type="java.lang.Integer">
			<column name="type" length="2" />
		</property>
		<property name="url" type="java.lang.String">
			<column name="url" length="2" />
		</property>
	</class>
</hibernate-mapping>