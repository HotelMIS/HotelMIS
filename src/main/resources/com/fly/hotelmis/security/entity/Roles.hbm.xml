<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.fly.hotelmis.security.entity.Roles" table="roles"
		catalog="finance">
		<id name="id" type="java.lang.String" column="id" length="32">
			<generator class="uuid.hex" />
		</id>
		<property name="rolename" type="java.lang.String">
			<column name="rolename" length="32" />
		</property>
		<property name="enable" type="java.lang.Integer">
			<column name="enable" length="2" />
		</property>
	</class>
</hibernate-mapping>